{
  "db_name": "PostgreSQL",
  "query": "\n            SELECT communities.*\n            FROM communities\n            LEFT JOIN posts ON communities.id = posts.community_id\n            WHERE communities.id IN (\n                SELECT DISTINCT community_id\n                FROM posts\n                WHERE author_id = $1\n            ) OR communities.owner_id = $1\n            GROUP BY communities.id\n            ORDER BY MAX(posts.published_at) DESC\n        ",
