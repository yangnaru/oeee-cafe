  "db_name": "PostgreSQL",
  "query": "\n        SELECT\n            comments.id,\n            comments.post_id,\n            comments.user_id,\n            comments.updated_at,\n            comments.created_at,\n            comments.content,\n            users.display_name AS user_display_name,\n            users.login_name AS user_login_name\n        FROM comments\n        LEFT JOIN users ON comments.user_id = users.id\n        WHERE post_id = $1\n        ORDER BY created_at DESC\n        ",
